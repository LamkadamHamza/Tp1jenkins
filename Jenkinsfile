pipeline{
    agent any
    
    stages{
        stage('hello'){
            steps{
                echo 'hello world'
            }
        }
        stage('dev'){
            steps{
                echo 'dev'
            }
        }
        stage('Test'){
            steps{
                echo 'Testing'
            }
        }
        stage('Deploy'){
            steps{
                echo 'Mis en prod'
            }
        }
    }
}
